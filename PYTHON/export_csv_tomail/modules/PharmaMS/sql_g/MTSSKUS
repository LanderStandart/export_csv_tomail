'{Year}',
'{MONTH}',
'{DAY}'

from
wares w
join parts p on p.ware_id=w.id and p.g$profile_id in ({profile_id})
where exists (select id from parts p where p.ware_id=w.id and p.g$profile_id in ({profile_id}))