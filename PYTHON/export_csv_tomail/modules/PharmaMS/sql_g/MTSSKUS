select
p.g$profile_id,
p.ware_id,
w.sname,
0,
0,
(select caption from agents a where a.id=(select agent_id from pr_getmotherpart(p.id, p.g$profile_id ) ))as OriginalStockLocation ,
3 as Replenishment,
p.price,
p.price_o,
p.price-p.price_o as Throughput,
1,
1,
0,
0,
0,
'',
'',
